{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.19.4",
    "pnpmVersion": "4.9.3",
    "pnpmOptions": {
        "strictPeerDependencies": true,
        "resolutionStrategy": "fast"
    },
    "nodeSupportedVersionRange": "^12.13.0",
    "ensureConsistentVersions": true,
    "projectFolderMinDepth": 2,
    "projectFolderMaxDepth": 2,
    "approvedPackagesPolicy": {
        "reviewCategories": [
            "production"
        ],
        "ignoredNpmScopes": [
            "@types"
        ]
    },
    "gitPolicy": {
        "versionBumpCommitMessage": "Applying package updates. [skip-ci]"
    },
    "repository": {
        "url": "https://github.com/mpiroc-org/tools.git"
    },
    "eventHooks": {
        "preRushInstall": [],
        "postRushInstall": [],
        "preRushBuild": [],
        "postRushBuild": []
    },
    "variants": [],
    "projects": [
        {
            "packageName": "@mpiroc-org/build-layer",
            "projectFolder": "packages/build-layer",
            "reviewCategory": "production",
            "shouldPublish": true,
            "versionPolicyName": "default"
        },
        {
            "packageName": "@mpiroc-org/generator-rush",
            "projectFolder": "packages/generator-rush",
            "reviewCategory": "production",
            "shouldPublish": true,
            "versionPolicyName": "default"
        }
    ]
}